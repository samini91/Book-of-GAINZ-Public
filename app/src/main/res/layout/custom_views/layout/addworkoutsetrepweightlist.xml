<?xml version="1.0" encoding="utf-8"?>

<FrameLayout android:layout_height="match_parent"
             android:layout_width="match_parent"
             android:background="@color/Grey"
             xmlns:android="http://schemas.android.com/apk/res/android"
             android:id="@+id/addworkoutsetrepweightlistroot"
    >
<LinearLayout
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:weightSum="1"
    android:orientation="vertical"
    >


<RelativeLayout

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/selectexercsieconfigurationId"
    android:clickable="true"
    android:layout_weight=".5"
    android:weightSum="1"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <com.example.sadiq.test.SetRepWeightConfigurationView.SetRepWeightConfigurationView
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:id="@+id/setrepweightconfigurationview"
        />

</RelativeLayout>

    <Button android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:id="@+id/SubmitButton"
            android:text="Submit"
        />
</LinearLayout>

    </FrameLayout>